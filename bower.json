{
  "name": "fedkit",
  "description": "A fast and stable Front End Development workflow. Built with Grunt & Libsass.",
  "private": "true",
  "homepage": "https://github.com/PJL101/fedkit",
  "authors": [
    "Phil Lennon <enquiry@iampjl.co.uk>"
  ],
  "moduleType": [
    "globals"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "susy": "~2.2.5",
    "modernizr": "~2.8.3",
    "sass-mq": "~3.1.2",
    "normalize-scss": "~3.0.3",
    "jquery": "2.x"
  },
  "devDependencies": {
    "tota11y": "~0.0.6"
  },
  "install": {
    "base": "bower_filtered",
    "path": {
      "js": "js/vendor/bower/{name}",
      "scss": "scss/vendor/bower/{name}",
      "css": "scss/vendor/bower/{name}",
      "eot": "fonts/{name}",
      "svg": "fonts/{name}",
      "ttf": "fonts/{name}",
      "woff": "fonts/{name}",
      "woff2": "fonts/{name}"
    },
    "ignore": [
      "modernizr"
    ],
    "sources": {
      "susy": "bower_components/susy/sass/**",
      "jquery": {
        "mapping": [
          {
            "bower_components/jquery/dist/jquery.js": "../_jquery.js"
          }
        ]
      }
    }
  }
}
